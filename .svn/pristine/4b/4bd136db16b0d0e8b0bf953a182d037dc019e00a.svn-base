     "input": [
      "#Net Present Value (NPV)\n",
      "#Example Call - npv(0.5,[-100,60,60,60])\n",
      "\n",
      "def npv(r,cashflow):\n",
      "    total = 0.0\n",
      "    for x in range(0,len(cashflow), 1):\n",
      "        total += cashflow[x]/(1+r)**x\n",
      "\n",
      "    print \"Net Present Value: {0:.4f}\".format(total)"
     ],
